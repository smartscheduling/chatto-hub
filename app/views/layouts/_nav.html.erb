<%# <div class="navbar-fixed"> %>
  <nav class="">
    <div class="nav-wrapper">
      <div class="row">
        <div class="col s8 offset-s2">
          <a href="/" class="brand-logo white-text">Chatto</a>

          <ul id="nav-mobile" class="right hide-on-med-and-down">
            <li><%= link_to 'New Project', new_project_path %></li>
            <li><%= link_to 'Browse Projects', projects_path unless current_page?(controller: "/projects", action: "index") %></li>
            <%- if current_user.authenticated? -%>
              <li><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %></li>
              <li id="nav-image"><%= image_tag(avatar_url(current_user, 64), size: "64x64") %></li>
            <%- else -%>
              <li><a class="modal-trigger" href="#modal1">Sign In</a></li>
            <%- end %>
          </ul>
        </div>
      </div>
    </div>
  </nav>

   <div id="modal1" class="modal">
    <div class="modal-content">
      <h3>Sign in with Github</h3>
      <p>We currently require sign in with github in order to give you access to project repositories.</p>
      <%= link_to omniauth_authorize_path(:user, :github), class: 'btn white-text' do %>
        <i class="fa fa-github fa-2x"></i>
        Sign in with Github
      <% end %>
    </div>
  </div>
<%# </div> %>
